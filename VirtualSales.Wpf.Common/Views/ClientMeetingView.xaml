<UserControl x:Class="VirtualSales.Wpf.Views.ClientMeetingView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewModels="clr-namespace:VirtualSales.Core.ViewModels;assembly=VirtualSales.Core"
             xmlns:views="clr-namespace:VirtualSales.Wpf.Views"
             mc:Ignorable="d" 
             d:DesignHeight="350" d:DesignWidth="600" d:DataContext="{d:DesignInstance viewModels:ClientMeetingViewModel}">
    <Grid>
        <ContentPresenter Content="{Binding Meeting}" ></ContentPresenter>
    </Grid>
</UserControl>
